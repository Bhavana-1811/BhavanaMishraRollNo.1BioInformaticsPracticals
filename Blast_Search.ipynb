    "from collections import Counter\n",
    "\n",
    "file1 = open(r\"E:\\MSC CS SEM 1\\BioInformatics\\Bio Informatics Practical\\H1N1_seq.txt\", \"r\")\n",
    "data = file1.read()\n",
    "\n",
    "count = Counter(data)\n",
    "\n",
    "for i, j in count.items():\n",
    "    if i != '\\n':\n",
    "        print(\"Count of \" + str(i) + \" is: \" + str(j))\n"
   ]
